{
    "variable": {
        "enable_deletion_protection": [
            {
                "default": false,
                "description": "If true, deletion of the load balancer will be disabled",
                "type": "bool",
                "nullable": true
            }
        ],
        "internal": [
            {
                "default": false,
                "description": "Whether the ALB should be internal",
                "type": "bool",
                "nullable": true
            }
        ],
        "load_balancer_type": [
            {
                "default": "application",
                "description": "The type of load balancer to create. Possible values are application or network",
                "type": "string",
                "nullable": true
            }
        ],
        "name": [
            {
                "description": "Name of the load balancer",
                "type": "string",
                "nullable": true
            }
        ],
        "security_groups": [
            {
                "description": "List of security group IDs to assign to the load balancer",
                "type": "list(string)",
                "nullable": true
            }
        ],
        "subnets": [
            {
                "description": "List of subnet IDs where the load balancer will be deployed",
                "type": "list(string)",
                "nullable": true
            }
        ],
        "tags": [
            {
                "default": {},
                "description": "A map of tags to add to all resources",
                "type": "map(string)",
                "nullable": true
            }
        ],
        "enable_http2":[
            {
                "default": true,
                "description": "If true, the LB will support HTTP/2",
                "type": "bool",
                "nullable": true
            }
        ],
        "access_logs": [
            {
                "description": "Configuration block for access logs",
                "type": "list(object({bucket: string, prefix: string, enabled: bool}))",
                "nullable": true
            }
        ]
    }
}