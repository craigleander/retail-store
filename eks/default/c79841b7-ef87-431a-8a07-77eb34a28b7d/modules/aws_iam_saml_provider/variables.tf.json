{
    "variable": {
        "name": [
            {
                "type": "string",
                "description": "The name of the provider to create.",
                "validation": {
                    "condition": "${length(var.name) >= 1 && length(var.name) <= 128}",
                    "error_message": "`name` must be between 1-128 characters in length."
                },
                "nullable": false
            }
        ],
        "saml_metadata_document": [
            {
                "type": "string",
                "description": "An XML document generated by an identity provider that supports SAML 2.0.",
                "nullable": false
            }
        ],
        "tags": [
            {
                "type": "map(string)",
                "description": "Key-value map of resource tags.",
                "nullable": true
            }
        ]
    }
}
