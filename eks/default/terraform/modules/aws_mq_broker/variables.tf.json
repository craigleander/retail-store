{
    "variable": {
        "broker_name": [
            {
                "type": "string",
                "nullable": false,
                "description": "Name of the broker."
            }
        ],
        "engine_type": [
            {
                "type": "string",
                "nullable": false,
                "description": "Type of broker engine."
            }
        ],
        "engine_version": [
            {
                "type": "string",
                "nullable": false,
                "description": "Version of the broker engine."
            }
        ],
        "host_instance_type": [
            {
                "type": "string",
                "nullable": false,
                "description": "Broker's instance type."
            }
        ],
        "user":[
            {
                "type": "list(object({username:string,password:string,console_access:bool,groups:list(string),replication_user:bool}))",
                "nullable": false,
                "description": "Username of the user."
            }
        ],
        "apply_immediately": [
            {
                "type": "bool",
                "default": false,
                "description": "Specifies whether any broker modifications are applied immediately, or during the next maintenance window.",
                "nullable": true
            }
        ],
        "authentication_strategy": [
            {
                "type": "string",
                "default": "simple",
                "description": "Authentication strategy used to secure the broker. Valid values are simple and ldap. ldap is not supported for engine_type RabbitMQ.",
                "nullable": true
            }
        ],
        "configuration":[
            {
                "type": "list(object({id:string,revision:number}))",
                "nullable": true,
                "description": "Configuration of the broker."
            }
        ],
        "auto_minor_version_upgrade": [
            {
                "type": "bool",
                "nullable": true,
                "description": "Whether to automatically upgrade to new minor versions of brokers as Amazon MQ makes releases available."
            }
        ],
        "deployment_mode": [
            {
                "type": "string",
                "default": "SINGLE_INSTANCE",
                "description": "Deployment mode of the broker.",
                "nullable": true
            }
        ],
        "encryption_options":[
            {
                "type": "list(object({use_aws_owned_key:bool,kms_key_id:string}))",
                "nullable": true,
                "description": "Encryption options for the broker."
            }
        ],
        "ldap_server_metadata":[
            {
                "type": "list(object({hosts:list(string),role_base:string,role_name:string,role_search_matching:string,role_search_subtree:string,user_base:string,user_role_name:string,user_search_matching:string,user_search_subtree:string,service_account_username:string,service_account_password:string}))",
                "nullable": true,
                "description": "Metadata of the LDAP server."
            }
        ],
        "logs":[
            {
                "type": "list(object({general:bool,audit:bool}))",
                "nullable": true,
                "description": "List of log types to enable. Valid values are general and audit. Audit logging is only possible for engine_type of ActiveMQ."
            }
        ],
        "maintenance_window_start_time":[
            {
                "type": "list(object({day_of_week:string,time_of_day:string,time_zone:string}))",
                "nullable": true,
                "description": "The start time of the maintenance window."
            }
        ],
        "publicly_accessible": [
            {
                "type": "bool",
                "default": false,
                "description": "Whether to enable connections from applications outside of the VPC that hosts the broker's subnets.",
                "nullable": true
            }
        ],
        "security_groups": [
            {
                "type": "list(string)",
                "nullable": true,
                "description": "List of security group IDs assigned to the broker."
            }
        ],
        "subnet_ids": [
            {
                "type": "list(string)",
                "nullable": true,
                "description": "List of subnet IDs in which to launch the broker. A SINGLE_INSTANCE deployment requires one subnet. An ACTIVE_STANDBY_MULTI_AZ deployment requires multiple subnets."
            }
        ],
        "storage_type": [
            {
                "type": "string",
                "nullable": true,
                "description": "Storage type of the broker. For engine_type ActiveMQ, the valid values are efs and ebs, and the AWS-default is efs. For engine_type RabbitMQ, only ebs is supported. When using ebs, only the mq.m5 broker instance type family is supported."
            }
        ],
        "tags": [
            {
                "default": {},
                "description": "A mapping of AWS tags to assign.",
                "type": "map(string)",
                "nullable": false
            }
        ]
    }
}